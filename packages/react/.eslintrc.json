{
    "env": {
        "browser": true,
        "es2022": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:react/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": [
        "dist",
        "lib/components/stencil-generated"
    ],
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "rules": {}
}
